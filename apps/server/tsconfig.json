{
  "compilerOptions": {
    "target": "ES2022",
    "outDir": "dist",
    "module": "commonjs",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
  },
  "references": [{"path": "../../shared/schemas"}]
}
